{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Port 3000)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": ["--web-port", "3000"]
    },
    {
      "name": "Flutter Web (Incognito)",
      "type": "dart",
      "request": "launch",
